<%- include('partials/header') %>

<form action="/notes/store" method="post" enctype="multipart/form-data">
<div class="form-group">
    <label for="title">Title:</label>
    <input class="form-control" type="text" name="title" id="title">
</div>

<div class="form-group">
    <label for="image">Image:</label>
    <input class="form-control-file" type="file" name="image" id="image" onchange="test()">
</div>

<div class="form-group">
    <label for="notes">Notes:</label>
    <textarea class="form-control" name="notes" id="notes" cols="30" rows="10"></textarea>
</div>

<button class="btn px-4 btn-success">Submit</button>
</form>


<%- include('partials/footer') %> 